<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @restaurant_place.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @restaurant_place.address %>
</p>

<p><strong>Items:</strong></p>
  <ul>
    <% @restaurant_place.reviews.each do |review| %>
    <li>
      <%= link_to review.item.name, item_url(review.item.id)  %>
    </li>
    <% end %>
  </ul>


<%= link_to 'Edit', edit_restaurant_place_path(@restaurant_place) %> |
<%= link_to 'Back', restaurant_places_path %>
